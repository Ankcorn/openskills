	},
	"dependencies": {
		"@hono/standard-validator": "^0.2.2",
		"@tailwindcss/cli": "^4.1.18",
		"@tailwindcss/typography": "^0.5.19",
		"better-result": "^2.3.0",
		"hatchlet": "^0.0.4",
		"hono": "^4.11.4",
